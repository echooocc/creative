div.clock
  h1.header CLOCKY
  div.linebar
  div.linebar
  div.phone
    h6.phone-title PHONE NUMBER
    div.phone-number 416-268-1649
  div.clocky
    div.date
      select#month(ng-model="selectMonth" ng-options="month as month.name for month in months")
      select#day(ng-module="day")
        option(value='') 22
